   "source": [
    "DMSO is overrepresented in the training set. We will correct for this by sampling 192 random vectors with this MoA (Remark: DMSO is the negative control, so it essentialy means no MoA)."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "a09e4faa",
   "metadata": {},
   "outputs": [],
   "source": [
    "def balance_training_set(df_train, final_number):\n",
    "    \"\"\"Reduces the number of DMSO datapoints in the training set to ensure it is balanced with other classes\n",
    "    Arguments:\n",
    "        - df_train: pd.DataFrame object. The dataframe contaiing the training set\n",
    "        - final_number: integer. The number of datapoints in the 'DMSO' class we want to keep\"\"\"\n",
    "\n",
    "    #first we get the indices of the rows containing DMSO-obtained embeddings\n",
    "    idx_list = df_train[df_train.Metadata_MoA == 'DMSO'].index\n",
    "\n",
    "    #we randomly sample 192 of those indices\n",
    "    np.random.seed(seed=0)\n",
    "    idx_subset = np.random.choice(idx_list, size = final_number, replace=False)\n",
    "\n",
    "    #and we filter the dataframe according to this subset\n",
    "    df_train = df_train[(df_train.Metadata_MoA != 'DMSO') | (df_train.index.isin(idx_subset))]\n",
    "    \n",
    "    return df_train\n",
    "\n",
    "df_train = balance_training_set(df_train, 192)"
   ]
